<?xml version="1.0" encoding="UTF-8"?>
<!--
Notes: These is the xml for a 10GbE Gauge
-->

<Widget Type = "SteelGauge">
    <Style>Gauge10GbE.css</Style>
    <MinValue>0</MinValue>
    <MaxValue>40</MaxValue>
    <UnitText>Gbps</UnitText>
    <Decimals>1</Decimals>
    <DialStartAngle>300</DialStartAngle>
    <DialRangeAngle>240</DialRangeAngle>
    <MajorTicksSpace>10</MajorTicksSpace>
    <MinorTicksSpace>5</MinorTicksSpace>
    <TickLableOrientation>Orthogonal</TickLableOrientation>
    <EnhancedRateText>False</EnhancedRateText>
    <Shadowed>True</Shadowed>
    <ShowMaxMeasuredValue>False</ShowMaxMeasuredValue>
    <ShowMinMeasuredValue>False</ShowMinMeasuredValue>
    
    <Sections>
        <Section start="0" end="10" />
        <Section start="10" end="20" />
        <Section start="20" end="30" />
        <Section start="30" end="40" />
    </Sections>
    
</Widget>
