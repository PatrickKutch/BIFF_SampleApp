			<MinionSrc Namespace="DemoNamespace" ID="(.*)"></MinionSrc>
			<ExcludeList>CPU_LIST;CPU_LIST2</ExcludeList>
		</Widget>
		<Widget File="Quick/QuickViewLCD.xml" row="$(NextRowAlias)" Column="$(CurrentColumnAlias)" hgap="5" vgap="5">
			<MinionSrc Namespace="DemoNamespace" ID="(.*)"></MinionSrc>
			<ExcludeList>CPU_LIST;CPU_LIST2</ExcludeList>
		</Widget>
	</Tab>
</MarvinExternalFile>
