		<Title>Quick View</Title>

		<Widget File="Text\Text.xml" row="$(CurrentRowAlias)" column="$(CurrentColumnAlias)">
			<InitialValue>Quick View Ability - uses RegEx filtering</InitialValue>
			<StyleOverride ID="Title"/>
